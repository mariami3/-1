@model proctos.Models.CategoryOfGender

@{
    ViewBag.Title = "Add Category Of Gender";
}

<h1>Add Category Of Gender</h1>

<form asp-action="AddCategoryOfGender" method="post">
    <div class="form-group">
        <label for="NameCategoryOfGender">Name</label>
        <input type="text" class="form-control" id="NameCategoryOfGender" name="NameCategoryOfGender" value="@Model.NameCategoryOfGender" required>
    </div>
    <button type="submit" class="btn btn-success">Add</button>
    <a href="@Url.Action("CategoryOfGenders", "Admin")" class="btn btn-secondary">Cancel</a>
</form>
